<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <version>2</version>

    <readers>
        <reader>
            <dll>gadgetron_core_readers</dll>
            <classname>AcquisitionReader</classname>
        </reader>
        <reader>
            <dll>gadgetron_core_readers</dll>
            <classname>WaveformReader</classname>
        </reader>
    </readers>

    <writers>
        <writer>
            <dll>gadgetron_core_writers</dll>
            <classname>ImageWriter</classname>
        </writer>
    </writers>

    <stream>
        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>NoiseAdjustGadget</classname>
        </gadget>

        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>PCACoilGadget</classname>
        </gadget>

        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>CoilReductionGadget</classname>
            <property name="coils_out" value="12"/>
        </gadget>

        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>AcquisitionAccumulateTriggerGadget</classname>
        </gadget>

        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>BucketToBufferGadget</classname>
            <property name="N_dimension" value="repetition"/>
            <property name="split_slices" value="false"/>
        </gadget>

        <external>
            <execute name="gadgetron.examples.IDEAL" type="matlab"/>
            <configuration/>
        </external>

        <gadget>
            <dll>gadgetron_gpuparallelmri</dll>
            <classname>gpuBufferSensePrepGadget</classname>
            <property name="profiles_per_frame" value="1"/>
        </gadget>

        <gadget>
            <dll>gadgetron_gpuparallelmri</dll>
            <classname>gpuCgSenseGadget</classname>
            <property name="number_of_iterations" value="30"/>
            <property name="oversampling_factor" value="2"/>
            <property name="kernel_width" value="7.5"/>
            <property name="output_convergence" value="true"/>
            <property name="save_individual_frames" value="false"/>
        </gadget>

        <gadget>
            <dll>gadgetron_mricore</dll>
            <classname>ExtractGadget</classname>
        </gadget>
    </stream>
</configuration>
